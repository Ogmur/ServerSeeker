  "authors": [
    "The Fifth Column"
  ],
  "contact": {
    "repo": "https://github.com/Ogmur/ServerSeeker/",
    "discord": "https://discord.gg/fabricmc",
    "homepage": "https://serverseeker.net/",
    "issues": "https://github.com/Ogmur/ServerSeeker/issues",
    "sources": "https://github.com/Ogmur/ServerSeeker/"
